<?xml version="1.0" encoding="utf-8"?>
<manifest
	package="com.nhpatt.screensapplication"
	xmlns:android="http://schemas.android.com/apk/res/android">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:theme="@style/AppTheme">
		<activity
			android:name=".MainActivity"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>

				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity
			android:name=".DDLFormActivity"
			android:label="@string/title_activity_ddlform"/>
		<activity
			android:name=".DDLListActivity"
			android:label="@string/title_activity_ddllist"/>
		<activity
			android:name=".WebContentActivity"
			android:label="@string/title_activity_web_content"/>
	</application>

</manifest>
