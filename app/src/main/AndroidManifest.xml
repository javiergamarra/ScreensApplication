<?xml version="1.0" encoding="utf-8"?>
<manifest
	package="com.nhpatt.screensapplication"
	xmlns:android="http://schemas.android.com/apk/res/android">

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:theme="@style/AppTheme">
		<activity
			android:name=".AutoLoginActivity"
			android:noHistory="true">
			<intent-filter>
				<action android:name="android.intent.action.MAIN"/>
				<category android:name="android.intent.category.LAUNCHER"/>
			</intent-filter>
		</activity>
		<activity android:name=".LoginActivity"/>
		<activity android:name=".DDLFormActivity"/>
		<activity android:name=".DDLListActivity"/>
		<activity android:name=".WebContentActivity"/>
		<activity android:name=".ForgotPasswordActivity"/>
		<activity android:name=".SignUpActivity"/>
		<activity android:name=".UserPortraitActivity"/>
		<activity android:name=".OAuthActivity"/>
	</application>

</manifest>
